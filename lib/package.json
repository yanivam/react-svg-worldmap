{
  "name": "react-svg-worldmap",
  "version": "1.1.0",
  "license": "MIT",
  "displayName": "React Worldmap",
  "description": "A pure react component to draw a map of world countries. Simple. Free.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "minify": "terser --compress --mangle --output ./dist/index.js -- ./dist/index.js && terser --compress --mangle --output ./dist/countries.geo.js -- ./dist/countries.geo.js",
    "prebuild": "rm -rf dist;eslint . --ext .tsx --fix",
    "build": "tsc -p tsconfig.json && yarn minify",
    "build:local": "yarn build && npm pack"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16.0.0"
  },
  "dependencies": {
    "d3-geo": "^1.11.9",
    "react-path-tooltip": "^1.0.17"
  },
  "devDependencies": {
    "@types/d3-geo": "^1.12.1",
    "@types/geojson": "^7946.0.7",
    "terser": "^5.3.3"
  },
  "authors": [
    "yanivam (https://github.com/yanivam)"
  ],
  "keywords": [
    "react",
    "worldmap",
    "world map",
    "map",
    "component",
    "typescript",
    "svg",
    "countries"
  ],
  "module": "dist/react-svg-worldmap.esm.js",
  "bugs": "https://github.com/yanivam/react-svg-worldmap/issues",
  "repository": "github:yanivam/react-svg-worldmap"
}
